@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rti:  <http://methc.la/lv2/ext/rt-instantiate#> .

<http://methc.la/lv2/plugins/sine>
    a lv2:Plugin ;
    doap:maintainer [
        foaf:name "Stefan Kersten" ;
        foaf:homepage <http://samplecount.com/team#stefan> ;
        foaf:mbox <mailto:stefan@samplecount.com>
    ] ;
    doap:name "Sine Wave Generator" ,
        "Sinuswellengenerator"@de ,
        "Generador de ondas sinusoidales"@es ;
    doap:license <http://opensource.org/licenses/isc> ;
    lv2:optionalFeature lv2:hardRTCapable
                      , rti:rtInstantiation ;
    lv2:extensionData rti:Interface ;
    lv2:port [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 0 ;
        lv2:symbol "freq" ;
        lv2:name "Frequency" ,
            "Frequenz"@de ,
            "Frecuencia"@es ;
        lv2:default 440.0 ;
        lv2:minimum 20.0 ;
        lv2:maximum 20000.0 ;
        lv2:scalePoint [
            rdfs:label "A4" ;
            rdf:value 440.0
        ]
    ] , [
        a lv2:AudioPort ,
            lv2:OutputPort ;
        lv2:index 1 ;
        lv2:symbol "out" ;
        lv2:name "Out"
    ] .
