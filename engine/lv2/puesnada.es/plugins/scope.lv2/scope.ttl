@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rti:  <http://mescaline.puesnada.es/lv2/ext/rt-instantiate#> .

<http://mescaline.puesnada.es/lv2/plugins/scope>
    a lv2:Plugin ;
    doap:maintainer [
        foaf:name "PuesNada inc" ;
        foaf:homepage <http://mescaline.puesnada.es/> ;
        foaf:mbox <mailto:devot@puesnada.es>
    ] ;
    doap:name "Oscilloscope" ,
        "Oszilloskop"@de ,
        "Osciloscopo"@es ;
    doap:license <http://opensource.org/licenses/isc> ;
    lv2:optionalFeature lv2:hardRTCapable
                      , rti:rtInstantiation ;
    lv2:extensionData rti:Interface ;
    lv2:port [
        a lv2:InputPort ,
            lv2:AudioPort ;
        lv2:index 0 ;
        lv2:symbol "in" ;
        lv2:name "In" ;
    ] .
